.indicator_column(\n","    tf.feature_column.categorical_column_with_hash_bucket(\n","      \"source\", hash_bucket_size=1000))"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"PArjS06d4ldD","colab_type":"code","colab":{}},"source":["# HYPERPARAMETERS\n","HIDDEN_UNITS = [500, 500, 500, 500, 500]\n","LEARNING_RATE = 0.08\n","L2_REGULARIZATION_STRENGTH = 0.6\n","NUM_STEPS = 10000"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"tBC4vzQ_4ewJ","colab_type":"code","colab":{}},"source":["# Putting it all together\n","estimator = tf.estimator.DNNClassifier(\n","    hidden_units=HIDDEN_UNITS,\n","    feature_columns=[embedded_title_feature_column, embedded_text_feature_column, source_feature_column],\n","    n_classes=2,\n","    batch_norm=True,\n","    optimizer=tf.train.ProximalAdagradOptimizer(\n","      learning_rate=LEARNING_RATE,\n","      l2_regularization_strength=L2_REGULARIZATION_STRENGTH\n","    ))"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"ituFGI_4q475","colab_type":"code","outputId":"d58102f2-6162-420b-e6be-0439ffbb56d6","executionInfo":{"status":"ok","timestamp":1572928172728,"user_tz":480,"elapsed":179798,"user":{"displayName":"Divya Sudhakar","photoUrl":"https://lh3.googleusercontent.com/a-/AAuE7mBAEe8lSbL-gO8W2QGjfXvHbFVw-wl2B28bQD3_6_A=s64","userId":"13661723492181317645"}},"colab":{"base_uri":"https://localhost:8080/","height":51}},"source":["estimator.train(input_fn=train_input_fn, steps=NUM_STEPS);\n","train_eval_result = estimator.evaluate(input_fn=predict_train_input_fn)\n","dev_eval_result = estimator.evaluate(input_fn=predict_dev_input_fn)\n","\n","print(\"Training set accuracy: {accuracy}\".format(**train_eval_result))\n","print(\"Dev set accuracy: {accuracy}\".format(**dev_eval_result))"],"execution_count":0,"outputs":[{"output_type":"stream","text":["Training set accuracy: 0.8815000057220459\n","Dev set accuracy: 0.8500000238418579\n"],"name":"stdout"}]},{"cell_type"